---
import ReturnHome from "../components/NavigationBar/ReturnHome.astro";
import GeneralContainerLayout from "../layouts/GeneralContainerLayout.astro";
import Layout from "../layouts/Layout.astro";

const pageTitle = "Contact"
---



<Layout pageTitle={pageTitle}>
    <GeneralContainerLayout>
        <div class="col-span-6 hidden md:block"></div>
        <div class="col-span-12 md:col-span-6">
            <h1 transition:name={pageTitle} 
            class="text-bold text-4xl md:text-6xl text-white text-right">{pageTitle}</h1>
        </div>
        <ReturnHome/>
    </GeneralContainerLayout>
</Layout>